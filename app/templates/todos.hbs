<section id="todoapp">
  <header id="header">
    <h1>todos</h1>

    {{input type="text" id="new-todo"
        placeholder="What needs to be done?"
        enter="addTodoItem"
        autofocus="autofocus"}}

  </header>

  <section id="main">
    {{todos-list todos=model}}
    <input type="checkbox" id="toggle-all">
  </section>
  {{todos-status todos=model}}

</section>
